                  label(for='santa-fe') Santa FE
                li.calc-dropdown__item
                  input#palisade(type='radio', name='model-item', value='palisade')
                  label(for='palisade') Palisade
                li.calc-dropdown__item
                  input#h1(type='radio', name='model-item', value='h1')
                  label(for='h1') H1
            li.calc-filter__tabs-item
