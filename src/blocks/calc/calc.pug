section.calc
  .container
    ul.calc__tabs
      li.calc__tabs-item.active
        label(for='reglament-tab-input') Прохождение ТО
      li.calc__tabs-item
        label(for='oil-tab-input') Масляный сервис
    .calc-filter
      .calc-filter__left
        .calc-result
          span.calc-result__head Стоимость
          p.calc-result__price 2375
          button.hyundai-button.hyundai-button--orange(type='button') &Zcy;&acy;&kcy;&acy;&zcy;&acy;&tcy;&softcy; &zcy;&vcy;&ocy;&ncy;&ocy;&kcy;
      .calc-filter__right
        input#reglament-tab-input.calc-tab-input(type='radio', name='calc-tab', checked='')
        .reglament-tab
          ul.calc-filter__tabs
            li.calc-filter__tabs-item
              input#reglament-model(type='radio', name='reglament-category', checked='')
              label.tab-label(for='reglament-model')
                span Модель
                .toggle
              ul.calc-dropdown
                li.calc-dropdown__item
                  label(for='creta') Creta
                  input#creta(type='radio', name='model-item', value='creta')
                li.calc-dropdown__item
                  input#tucson(type='radio', name='model-item', value='tucson')
                  label(for='tucson') Tucson
                li.calc-dropdown__item
                  input#solaris(type='radio', name='model-item', value='solaris')
                  label(for='solaris') Solaris
                li.calc-dropdown__item
